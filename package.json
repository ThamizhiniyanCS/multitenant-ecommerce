    "@types/react-dom": "19.1.2",
    "eslint": "^9.25.1",
    "eslint-config-next": "15.3.0",
    "prettier": "^3.5.3",
    "tw-animate-css": "^1.2.8",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
